{
  "$schema": "https://railway.app/railway.schema.json",
  "name": "worker",
  "description": "RQ Worker service for processing reports",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python -m worker.run",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "healthcheckPath": "",
    "healthcheckTimeout": 0
  }
}